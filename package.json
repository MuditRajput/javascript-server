  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.0",
    "nodemon": "^2.0.6",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "tslint": "^6.1.3"
  },
  "husky": {
    "hooks": {
